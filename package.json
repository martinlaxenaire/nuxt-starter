{
    "name": "@rezo-zero/nuxt-starter",
    "version": "0.1.0",
    "description": "Starter template for Nuxt project",
    "engines": {
        "node": "^16"
    },
    "scripts": {
        "dev": "nuxt",
        "build": "nuxt build",
        "start": "nuxt start",
        "generate": "nuxt generate",
        "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
        "lint:style": "stylelint \"**/*.{vue,css,scss}\" --ignore-path .gitignore",
        "lint": "yarn lint:js && yarn lint:style",
        "lint-fix": "yarn lint:js --fix && yarn lint:style --fix",
        "lint-staged": "lint-staged",
        "storybook": "nuxt storybook -c nuxt-storybook.config.ts -s ./src/assets/storybook",
        "storybook:build": "nuxt storybook build -c nuxt-storybook.config.ts -s ./src/assets/storybook",
        "xilo": "node ./node_modules/@rezo-zero/xilofone-fetch/dist/index.js",
        "analyze": "nuxt build --analyze",
        "postinstall": "husky install"
    },
    "dependencies": {
        "@nuxt/image": "^0.6.2",
        "@nuxtjs/recaptcha": "^1.1.1",
        "@nuxtjs/sentry": "^5.1.0",
        "@nuxtjs/sitemap": "^2.4.0",
        "@nuxtjs/svg": "^0.4.0",
        "@rezo-zero/intervention-request-provider": "^0.3.2",
        "@roadiz/nuxt-module": "^3.0.1",
        "body-scroll-lock": "3.x",
        "core-js": "^3.15.1",
        "date-fns": "^2.29.3",
        "lodash": "^4.17.21",
        "marked": "^4.2.2",
        "nuxt": "^2.17.0",
        "nuxt-i18n": "^6.27.2",
        "plyr": "^3.7.2",
        "throttle-debounce": "^3.0.1",
        "v-calendar": "^2.4.1",
        "vue-multiselect": "^2.1.6",
        "vue-plausible": "^1.2.0"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.14.7",
        "@nuxt/types": "^2.17.0",
        "@nuxt/typescript-build": "^3.0.1",
        "@nuxtjs/eslint-config-typescript": "^12.0.0",
        "@nuxtjs/eslint-module": "^3.0.2",
        "@nuxtjs/storybook": "^4.3.2",
        "@nuxtjs/style-resources": "^1.2.0",
        "@nuxtjs/stylelint-module": "^4.0.0",
        "@rezo-zero/xilofone-fetch": "^0.1.1",
        "@types/body-scroll-lock": "^3.1.0",
        "@types/json-schema": "^7.0.11",
        "@types/lodash": "^4.14.171",
        "@types/marked": "^4.0.7",
        "@types/throttle-debounce": "^2.1.0",
        "@vue-a11y/skip-to": "^2.1.2",
        "defu": "^6.1.0",
        "eslint": "^8.44.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-nuxt": "^2.0.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-vue": "^7.12.1",
        "husky": "^8.0.3",
        "include-media": "^1.4.10",
        "lint-staged": "^13.1.0",
        "nuxt-storm": "^1.1.3",
        "prettier": "^2.3.2",
        "sass": "^1.35.1",
        "sass-loader": "10",
        "stylelint": "^13.13.1",
        "stylelint-config-css-modules": "^2.2.0",
        "stylelint-config-idiomatic-order": "^8.1.0",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-standard": "^22.0.0",
        "stylelint-order": "^4.1.0",
        "stylelint-scss": "^3.19.0",
        "ts-node": "^10.9.1",
        "typescript": "4",
        "vue-typed-mixins": "^0.2.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rezozero/nuxt-starter.git"
    },
    "lint-staged": {
        "*.{js,ts,vue}": "eslint --ignore-path .gitignore"
    },
    "keywords": [
        "nuxt",
        "roadiz",
        "nuxtjs",
        "starter",
        "template"
    ],
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/rezozero/nuxt-starter/issues"
    },
    "homepage": "https://github.com/rezozero/nuxt-starter#readme"
}
